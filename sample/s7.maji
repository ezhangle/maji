printf :: (fmt: char*, f: f64) #foreign "/usr/lib/libc.dylib";

a :: 1.5;
b := 2.5;
main :: () -> int {
    c := 7.5;
    b = 3.7;
    d := 5;
    c = a + b;
    printf("a is %.2lf\n", a + 1.1);
    printf("b is %.2lf\n", b);
    printf("c is %.2lf\n", c);

    printf("b + -2.0 is %.2lf\n", b + -2.0);
    return d + 3;
}
