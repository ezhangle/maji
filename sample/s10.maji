printf :: (fmt: char*, a: int, b: float) #foreign "/usr/lib/libc.dylib";
main :: () {
    n : int = 0;
    m: float[3];
    m[n] = 3.5;
    m[n+1] = 1.9;
    m[n+2] = 6.3;

    printf("m[%d] = %.2f\n", n, m[n]);
    printf("m[%d] = %.2f\n", n+1, m[n+1]);
    printf("m[%d] = %.2f\n", n+2, m[n+2]);
}
